<h1>YOR TASKS</h1>
<ul>
  <% @tasks.each do |task| %>
    <!-- link_to "ANCHOR", path -->
    <li><%= task.completed %></li>
    <li><%= link_to task.title, task_path(task.id) %></li>
    <p><%= link_to "Delete the task", delete_task_path(task.id), data: {confirm: "Are you sure?" }, method: :delete %></p>
  <% end %>
  <%= link_to "Create new task",tasks_new_path %>
</ul>